{
  "plugins": [
    "syntax-dynamic-import",
    "transform-class-properties"
  ],
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current",
          "browsers": "last 2 versions"
        },
        "modules": false,
        "loose": true
      }
    ],
    "react",
    "flow"
  ],
  "env": {
    "development": {
      "plugins": [
        "tcomb",
        "react-hot-loader/babel",
        "babel-plugin-tcomb"
      ]
    },
    "production": {
      "plugins": [
        "transform-flow-strip-types"
      ]
    }
  }
}