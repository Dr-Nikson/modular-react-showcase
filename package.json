{
  "name": "au-ui",
  "version": "0.0.1",
  "description": "aliexpress goods",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf build/public && cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress --env.prod=true",
    "build-server": "rimraf build/private && cross-env NODE_ENV=development webpack --config ./webpack.config.js --progress --colors --display-error-details --env.node=true",
    "start-old": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack.config.js",
    "start": "cross-env NODE_ENV=development NODE_PATH=\"src\" yarn run build-server && node ./build/private/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://GameNik@bitbucket.org/bits-plz/au-ui.git"
  },
  "keywords": [
    "react",
    "webpack",
    "flow",
    "redux"
  ],
  "author": "Nik Nik",
  "license": "MIT",
  "homepage": "https://bitbucket.org/bits-plz/au-ui#readme",
  "dependencies": {
    "express": "^4.15.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "source-map-support": "^0.4.15"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^4.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "preload-webpack-plugin": "^1.2.2",
    "react-hot-loader": "3.0.0-beta.6",
    "rimraf": "^2.6.1",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^2.5.0",
    "webpack-bundle-analyzer": "^2.6.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-node-externals": "^1.6.0"
  }
}
